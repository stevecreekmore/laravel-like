{
    "name": "stevecreekmore/laravel-like",
    "description": "A User can like another User",
    "license": "MIT",
    "authors": [
        {
            "name": "Steven Creekmore",
            "email": "creekmore108@gmail.com",
            "homepage": "https://github.com/stevecreekmore"
        }
    ],
    "minimum-stability": "stable",
    "homepage": "https://github.com/stevecreekmore/laravel-like",
    "keywords": [
        "Laravel",
        "LLike",
        "Like users"
    ],
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "php": "^8.1",
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "~7"
    },
    "autoload": {
        "psr-4": {
            "stevecreekmore\\LaravelLike\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "stevecreekmore\\LaravelLike\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-f": "vendor/bin/phpunit --filter"
    },
    "extra": {
        "laravel": {
            "providers": [
                "stevecreekmore\\LaravelLike\\LLikeServiceProvider"
            ],
            "aliases": {
                "LBlock": "stevecreekmore\\LaravelLike\\Facades\\LaravelLike"
            }
        }
    }
}
